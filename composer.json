{
    "name": "blockmurder/pdfPreview",
    "description": "Generates a preview image form a PDF File with <code> {{ pdfpre( filename, width, hight ) }} </code>",
    "type": "bolt-extension",
    "keywords" : [
            "PDF",
            "preview"
    ],
    "require": {
        "bolt/bolt" : ">=2.0.0,<3.0.0"
    },
    "license": "MIT",
    "authors": [{"name": "blockmurder", "email": "info@blockmurder.ch"}
    ],
    "autoload": {
        "files": [
            "init.php"
        ],
        "psr-4": {
            "Bolt\\Extension\\blockmurder\\pdfPreview\\": ""
        }
    },
    "extra": {
        "branch-alias" : {
            "dev-master" : "dev-develop"
        }
    }
}
